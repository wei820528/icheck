                    }
                    //if (MonthCycleArray[i] != 0) {
                    //    if (Number(MonthCycleArray[i]) > 31) {
                    //        console.log(MonthCycleArray[i]);
                    //        msgtxt += "日期不能超過三一天:" + MonthCycleArray[i] + enter();
