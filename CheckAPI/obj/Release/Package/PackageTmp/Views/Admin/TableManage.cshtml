           // ^ [A - Za - z0 - 9\,] $
            var emailRegxp = /^([a-zA-Z0-9_\.\-\+])+\@@([a-zA-Z0-9]{2,4})+$/;
            if (emailRegxp.test(emailStr) != true) {
                msgtxt = "請輸入正確\n";
